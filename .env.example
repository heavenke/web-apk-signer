# Android 签名服务配置文件 (.env)
# 请复制为 .env 并填入实际值
# 文件路径: /opt/config/.env

KEYSTORE_PATH=/opt/keystore/your.jks
KEYSTORE_PASSWORD=password
KEY_ALIAS=your_alias
KEY_PASSWORD=your_alias_password
JAVA_PATH=/usr/bin/java
APKSIGNER_JAR=/opt/android-sdk/build-tools/34.0.0/lib/apksigner.jar
ZIPALIGN_PATH=/opt/android-sdk/build-tools/34.0.0/zipalign
JARSIGNER_PATH=/usr/bin/jarsigner
TEMP_DIR=/tmp
AAPT_PATH=/opt/android-sdk/build-tools/34.0.0/aapt